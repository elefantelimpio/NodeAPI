{
  "name": "nodeapi",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register ./index.ts",
    "start:watch": "nodemon -r dotenv/config ./index.js",
    "build": "tsc",
    "start_local": "node -r dotenv/config ./index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "Pablo Guerrero Gonz√°lez",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/node": "^11.9.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fast-json-patch": "^2.0.7",
    "i18n": "^0.8.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.4.7",
    "morgan": "^1.9.1",
    "node-restful": "^0.2.6",
    "patch-package": "^6.0.2",
    "request": "^2.88.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/i18n": "^0.8.5",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.3.17",
    "@types/morgan": "^1.7.35",
    "eslint": "^5.14.0",
    "eslint-plugin-import": "^2.16.0",
    "extract-mongo-schema": "^0.2.7",
    "nodemon": "^1.18.10"
  }
}
